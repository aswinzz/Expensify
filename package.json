    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "6.24.1",
    "css-loader": "^0.28.11",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "4.0.0-alpha.0",
    "live-server": "^1.2.0",
    "moment": "^2.21.0",
    "node-sass": "^4.8.3",
    "normalize.css": "^8.0.0",
    "react": "16.0.0",
    "react-addons-shallow-compare": "^15.6.2",
    "react-dates": "12.2.4",
    "react-dom": "16.0.0",
    "react-modal": "^3.3.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
